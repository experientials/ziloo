<script type="text/javascript">
// <![CDATA[
  var parent = "";
  var changed = false;
  var ajax = false;
  
  $(function() {                                                                                                          
        $( "#dialog").dialog({
            autoOpen: false,
            resizable: false,
            width:440,
            modal: true,
            buttons: {
                "Leave page": function() {                                                                        
                    changed = false;    
                    if(!ajax){                          
                      if($(parent).is("a")) {
                          window.location = $(parent).attr("href");
                      }                          
                      if($(parent).is("form")) {
                          $(parent).submit();
                      }                
                    }else{
                      $(parent).click();    
                    }       
                    
                    ajax = false;                                                                                                    
                    $(this).dialog("close"); 
                },
                "Stay on page": function() {
                    $(this).dialog("close");
                }
            }                                                      
        });
        
        // dirty fix for all modals
        var ids = [];
        setTimeout(function(){
            $('.ui-dialog.ui-widget.ui-widget-content').each(function(i){
                ids[i] = $(this).attr('aria-describedby');
            });    

            var selector = '#' + ids.join(', #');

            $(selector).on('dialogopen',function(){
                $('.mm-slideout').css('z-index', 'unset');
            });
            
            $(selector).on('dialogclose',function(){
                $('.mm-slideout').css('z-index', '1');
            });
        });
        
   });      
    
// ]]>
</script> 
