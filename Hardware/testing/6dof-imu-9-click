          <script>

$(document).ready(function () {
    (function () {
        var dialog = $('#license_dialog');

        $(dialog).dialog({
            autoOpen: false,
            resizable: false,
            width: 440,
            modal: true,
            buttons: {
                'OK': function () {
                    $(this).dialog('close');
                    $('.mm-slideout').css('z-index', '1');
                }
            }
        });

        $('.project-license-link').on('click', function () {
          if($(this).attr('href') == 'javascript:void(0)'){
            if (!$(dialog).dialog('isOpen')) {
              $('.mm-slideout').css('z-index', 'unset');
                $(dialog).dialog('open');
            }
          }
        });

    })();
});
</script>
