<div id="jstree_demo_div"></div>
<script>
$(document).ready(function(){
    $('#categories-js-tree').jstree({
        core: {
            dont_load_ajax_initially: true,
            load_open: false,
            check_callback: false,
            themes: {
                stripes: false
            },
            expand_selected_onload: false,
            data: {
                cache: false,
                dataType: 'json',
                url: function(node) {
                    var categoryIdString = '';
                    if (node.id != '#') {
                        categoryIdString = '/' + node.id;
                    }
                    return ('/project_categories/fetch_category_children' + categoryIdString);
                },
                success: function(new_data) {
                    return new_data;
                },
                data: function(node) {
                    return {
                        id: node.id
                    };
                },
                dataFilter: function(categories) {
                    var data = JSON.parse(categories)
                        .map(function(category) {
                            return category.ProjectCategory;
                        })
                        .map(function(category) {
                            return {
                                id: category.id,
                                text: category.name + ' (' + category.project_count + ')',
                                a_attr: {
                                    href: '/project_categories/view/' + category.id + '/' + category.slug
                                },
                                children: category.child_count > 0,
                                state: {
                                    opened: false,
                                    closed: true,
                                }
                            };
                        });
                    return JSON.stringify(data);
                }
            }
        },
        types: {
            default: {
                icon: false,
            },
        },
        plugins: [
            'state', 'types', 'wholerow',
            'themes', 'json_data', 'ui'
        ]
    }).on('activate_node.jstree', function(e, data) {
        window.location.href = data.node.a_attr.href;
    });
});
</script>
